{
  "name": "@yext/pages",
  "version": "1.0.0-beta.16",
  "description": "The default React development toolchain provided by Yext",
  "author": "sumo@yext.com",
  "license": "BSD-3-Clause",
  "keywords": [
    "Yext",
    "React"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./vite-plugin": "./dist/vite-plugin/plugin.js",
    "./util": "./dist/util/index.js",
    "./components": "./dist/components/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/src/index.d.ts"
      ],
      "vite-plugin": [
        "dist/types/src/vite-plugin/index.d.ts"
      ],
      "util": [
        "dist/types/src/util/index.d.ts"
      ],
      "components": [
        "dist/types/src/components/index.d.ts"
      ]
    }
  },
  "engines": {
    "node": "^17 || ^18 || ^19"
  },
  "scripts": {
    "watch": "node src/bundler --watch",
    "build:js": "node src/bundler.js",
    "build:types": "tsc",
    "build": "pnpm build:js && pnpm build:types",
    "fmt": "prettier --write --cache \"src/**/**.{ts,tsx,js,jsx}\"",
    "lint": "eslint --cache --fix src/**",
    "api-extractor": "api-extractor run --local --verbose",
    "generate-docs": "api-documenter markdown --input-folder temp --output-folder docs/api && rm -rf temp",
    "test": "jest",
    "generate-notices": "generate-license-file --input package.json --output ./THIRD-PARTY-NOTICES --overwrite",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006 -h localhost",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "prepare": "pnpm build"
  },
  "bin": {
    "pages": "dist/bin/spawn.js"
  },
  "files": [
    "dist/**/*",
    "THIRD-PARTY-NOTICES"
  ],
  "dependencies": {
    "@yext/analytics": "^0.3.0",
    "@yext/components-tsx-geo": "^1.0.3",
    "@yext/components-tsx-maps": "^1.0.9",
    "ansi-to-html": "^0.7.2",
    "browser-or-node": "^2.1.1",
    "chalk": "^5.0.1",
    "classnames": "2.3.1",
    "cli-spinners": "^2.7.0",
    "commander": "^9.4.0",
    "cross-fetch": "^3.1.5",
    "escape-html": "^1.0.3",
    "esm-module-paths": "^1.1.1",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "get-port": "^6.1.2",
    "glob": "^7.2.3",
    "handlebars": "^4.7.7",
    "ink": "^3.2.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "module-from-string": "^3.2.1",
    "open": "^8.4.0",
    "ora": "^6.1.2",
    "picocolors": "^1.0.0",
    "pretty-ms": "^7.0.1",
    "rollup": "^2.79.0",
    "shelljs": "^0.8.5",
    "typescript": "^4.8.2",
    "vite": "^3.1.0",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@microsoft/api-documenter": "^7.19.10",
    "@microsoft/api-extractor": "^7.30.0",
    "@storybook/addon-actions": "^6.5.11",
    "@storybook/addon-backgrounds": "^6.5.11",
    "@storybook/addon-docs": "^6.5.11",
    "@storybook/addon-essentials": "^6.5.11",
    "@storybook/addon-interactions": "^6.5.11",
    "@storybook/addon-links": "^6.5.11",
    "@storybook/addon-measure": "^6.5.11",
    "@storybook/addon-outline": "^6.5.11",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/client-api": "^6.5.11",
    "@storybook/client-logger": "^6.5.11",
    "@storybook/react": "^6.5.11",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.30",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.184",
    "@types/mime-types": "^2.1.1",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^17.0.45",
    "@types/react": "^17.0.49",
    "@types/react-dom": "^17.0.17",
    "@types/shelljs": "^0.8.11",
    "@types/ws": "^8.5.3",
    "@types/yargs": "^17.0.13",
    "babel-loader": "^8.2.5",
    "esbuild": "^0.15.7",
    "generate-license-file": "^1.3.0",
    "glob": "^7.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "minimatch": "^5.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^28.0.8"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "jest": {
    "rootDir": "..",
    "collectCoverageFrom": [
      "<rootDir>/pages/src/**/*.{ts,tsx,js}"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": {
          "allowSyntheticDefaultImports": true,
          "esModuleInterop": true,
          "allowJs": true,
          "target": "ESNext",
          "jsx": "react"
        }
      }
    },
    "verbose": true,
    "testMatch": [
      "<rootDir>/pages/src/**/*.test.{ts,tsx,js}"
    ],
    "transform": {
      "\\.[jt]sx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^(.+)\\.js$": "$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    }
  },
  "overrides": {
    "webpack": "^5.52.0"
  },
  "setupFiles": [
    "dotenv/config"
  ]
}
