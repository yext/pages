{
  "name": "@yext/pages",
  "version": "1.2.8",
  "description": "The default React development toolchain provided by Yext",
  "author": "sumo@yext.com",
  "license": "BSD-3-Clause",
  "keywords": [
    "Yext",
    "React"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/yext/pages.git"
  },
  "exports": {
    ".": "./dist/index.js",
    "./vite-plugin": "./dist/vite-plugin/plugin.js",
    "./util": "./dist/util/index.js",
    "./plugins": "./dist/plugins/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/src/index.d.ts"
      ],
      "vite-plugin": [
        "dist/types/src/vite-plugin/index.d.ts"
      ],
      "util": [
        "dist/types/src/util/index.d.ts"
      ],
      "plugins": [
        "dist/types/src/plugins/index.d.ts"
      ]
    }
  },
  "engines": {
    "node": "^20.6.0 || ^22 || ^24"
  },
  "scripts": {
    "watch": "node src/bundler --watch",
    "build:js": "node src/bundler.js",
    "build:types": "tsc",
    "build": "pnpm build:js && pnpm build:types",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 8080",
    "fmt": "prettier --write --cache \"src/**/**.{ts,tsx,js,jsx}\"",
    "lint": "oxlint --fix",
    "api-extractor": "api-extractor run --local --verbose",
    "generate-docs": "api-documenter markdown --input-folder temp --output-folder docs/api && rm -rf temp",
    "test": "vitest run",
    "generate-notices": "generate-license-file --input package.json --output ./THIRD-PARTY-NOTICES --overwrite",
    "prepare": "pnpm build"
  },
  "bin": {
    "pages": "dist/bin/spawn.js"
  },
  "files": [
    "dist/**/*",
    "THIRD-PARTY-NOTICES"
  ],
  "dependencies": {
    "ansi-to-html": "^0.7.2",
    "browser-or-node": "^2.1.1",
    "commander": "^12.1.0",
    "escape-html": "^1.0.3",
    "express": "^4.18.3",
    "fs-extra": "^11.2.0",
    "get-port": "^7.0.0",
    "glob": "^10.3.10",
    "latest-version": "^9.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "open": "^10.1.0",
    "ora": "^8.0.1",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.35",
    "postcss-nested": "^6.0.1",
    "pretty-ms": "^9.0.0",
    "prompts": "^2.4.2",
    "rollup": "^4.27.2",
    "ts-morph": "^22.0.0",
    "vite-plugin-node-polyfills": "0.17.0",
    "vitepress": "1.5.0",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.25.21",
    "@microsoft/api-extractor": "^7.47.11",
    "@types/escape-html": "^1.0.4",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.43",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.17.0",
    "@types/mime-types": "^2.1.4",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^20.11.28",
    "@types/prompts": "^2.4.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "esbuild": "^0.20.2",
    "generate-license-file": "^3.6.0",
    "glob": "^10.3.10",
    "jsdom": "24.0.0",
    "minimatch": "^9.0.3",
    "type-fest": "^4.27.0",
    "typescript": "^5.4.2",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.2.0",
    "react-dom": "^17.0.2 || ^18.2.0",
    "vite": "^4.3.0 || ^5.0.2"
  }
}