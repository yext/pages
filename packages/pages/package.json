{
  "name": "@yext/pages",
  "version": "1.0.0",
  "description": "The default React development toolchain provided by Yext",
  "author": "sumo@yext.com",
  "license": "BSD-3-Clause",
  "keywords": [
    "Yext",
    "React"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./vite-plugin": "./dist/vite-plugin/plugin.js",
    "./util": "./dist/util/index.js",
    "./plugins": "./dist/plugins/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/src/index.d.ts"
      ],
      "vite-plugin": [
        "dist/types/src/vite-plugin/index.d.ts"
      ],
      "util": [
        "dist/types/src/util/index.d.ts"
      ],
      "plugins": [
        "dist/types/src/plugins/index.d.ts"
      ]
    }
  },
  "engines": {
    "node": "^18 || ^20.2.0"
  },
  "scripts": {
    "watch": "node src/bundler --watch",
    "build:js": "node src/bundler.js",
    "build:types": "tsc",
    "build": "pnpm build:js && pnpm build:types",
    "fmt": "prettier --write --cache \"src/**/**.{ts,tsx,js,jsx}\"",
    "lint": "eslint --cache --fix src/**",
    "api-extractor": "api-extractor run --local --verbose",
    "generate-docs": "api-documenter markdown --input-folder temp --output-folder docs/api && rm -rf temp",
    "test": "vitest run",
    "generate-notices": "generate-license-file --input package.json --output ./THIRD-PARTY-NOTICES --overwrite",
    "prepare": "pnpm build"
  },
  "bin": {
    "pages": "dist/bin/spawn.js"
  },
  "files": [
    "dist/**/*",
    "THIRD-PARTY-NOTICES"
  ],
  "dependencies": {
    "ansi-to-html": "^0.7.2",
    "browser-or-node": "^2.1.1",
    "chalk": "^5.3.0",
    "cli-spinners": "^2.9.2",
    "commander": "^11.1.0",
    "cross-fetch": "^4.0.0",
    "escape-html": "^1.0.3",
    "esm-module-paths": "^1.1.1",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "get-port": "^7.0.0",
    "glob": "^10.3.10",
    "handlebars": "^4.7.8",
    "latest-version": "^7.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "module-from-string": "^3.3.0",
    "open": "^9.1.0",
    "ora": "^7.0.1",
    "picocolors": "^1.0.0",
    "pretty-ms": "^8.0.0",
    "prompts": "^2.4.2",
    "rollup": "^4.7.0",
    "ts-morph": "^21.0.1",
    "typescript": "^5.3.3",
    "vite-plugin-node-polyfills": "^0.17.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.23.17",
    "@microsoft/api-extractor": "^7.39.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/escape-html": "^1.0.4",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.41",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "4.0.9",
    "@types/lodash": "^4.14.202",
    "@types/mime-types": "^2.1.4",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^20.10.4",
    "@types/prompts": "^2.4.9",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/ws": "^8.5.10",
    "esbuild": "^0.19.8",
    "generate-license-file": "^3.0.0",
    "glob": "^10.3.10",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "23.0.1",
    "minimatch": "^9.0.3",
    "vitest": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.2.0",
    "react-dom": "^17.0.2 || ^18.2.0",
    "vite": "^4.3.0 || ^5.0.2"
  }
}
