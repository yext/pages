{
  "name": "@yext/pages",
  "version": "1.0.0-beta.4",
  "description": "The default React development toolchain provided by Yext",
  "author": "sumo@yext.com",
  "license": "BSD-3-Clause",
  "keywords": [
    "Yext",
    "React"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./vite-plugin": "./dist/vite-plugin/plugin.js",
    "./util": "./dist/util/index.js",
    "./components": "./dist/components/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/src/index.d.ts"
      ],
      "vite-plugin": [
        "dist/types/src/vite-plugin/index.d.ts"
      ],
      "util": [
        "dist/types/src/util/index.d.ts"
      ],
      "components": [
        "dist/types/src/components/index.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">=17"
  },
  "scripts": {
    "watch": "node src/bundler --watch",
    "build": "node src/bundler.js",
    "build:release": "pnpm run build && pnpm run types",
    "types": "tsc",
    "fmt": "prettier -w .",
    "lint": "eslint .",
    "api-extractor": "api-extractor run --local --verbose",
    "generate-docs": "api-documenter markdown --input-folder temp --output-folder docs/api && rm -rf temp",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs --port 8080",
    "test": "jest",
    "generate-notices": "generate-license-file --input package.json --output ./THIRD-PARTY-NOTICES --overwrite",
    "prepublishOnly": "pnpm run build:release",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "bin": {
    "pages": "dist/bin/pages.js"
  },
  "files": [
    "dist/**/*",
    "THIRD-PARTY-NOTICES"
  ],
  "dependencies": {
    "@yext/analytics": "^0.2.0",
    "ansi-to-html": "^0.7.2",
    "chalk": "^5.0.1",
    "classnames": "2.3.1",
    "cli-spinners": "^2.7.0",
    "commander": "^9.4.0",
    "cross-fetch": "^3.1.5",
    "escape-html": "^1.0.3",
    "esm-module-paths": "^1.1.1",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "glob": "^7.2.3",
    "handlebars": "^4.7.7",
    "ink": "^3.2.0",
    "lodash": "^4.17.21",
    "module-from-string": "^3.2.1",
    "open": "^8.4.0",
    "ora": "^6.1.2",
    "picocolors": "^1.0.0",
    "pretty-ms": "^7.0.1",
    "rollup": "2.77.2",
    "shelljs": "^0.8.5",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vitepress": "1.0.0-draft.8"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@microsoft/api-documenter": "^7.19.7",
    "@microsoft/api-extractor": "^7.29.5",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.30",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.184",
    "@types/node": "^17.0.45",
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.17",
    "@types/shelljs": "^0.8.11",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "babel-loader": "^8.2.5",
    "esbuild": "^0.15.5",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.0",
    "generate-license-file": "^1.3.0",
    "glob": "^7.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^28.0.8"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "jest": {
    "rootDir": "..",
    "collectCoverageFrom": [
      "<rootDir>/pages/src/**/*.{ts,tsx,js}"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": {
          "allowSyntheticDefaultImports": true,
          "esModuleInterop": true,
          "allowJs": true,
          "target": "ESNext",
          "jsx": "react"
        }
      }
    },
    "verbose": true,
    "testMatch": [
      "<rootDir>/pages/src/**/*.test.{ts,tsx,js}"
    ],
    "transform": {
      "\\.[jt]sx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^(.+)\\.js$": "$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    }
  }
}
