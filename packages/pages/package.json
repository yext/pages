{
  "name": "@yext/pages",
  "version": "1.1.0-beta.3",
  "description": "The default React development toolchain provided by Yext",
  "author": "sumo@yext.com",
  "license": "BSD-3-Clause",
  "keywords": [
    "Yext",
    "React"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./vite-plugin": "./dist/vite-plugin/plugin.js",
    "./util": "./dist/util/index.js",
    "./plugins": "./dist/plugins/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/src/index.d.ts"
      ],
      "vite-plugin": [
        "dist/types/src/vite-plugin/index.d.ts"
      ],
      "util": [
        "dist/types/src/util/index.d.ts"
      ],
      "plugins": [
        "dist/types/src/plugins/index.d.ts"
      ]
    }
  },
  "engines": {
    "node": "^18 || ^20.2.0"
  },
  "scripts": {
    "watch": "node src/bundler --watch",
    "build:js": "node src/bundler.js",
    "build:types": "tsc",
    "build": "pnpm build:js && pnpm build:types",
    "fmt": "prettier --write --cache \"src/**/**.{ts,tsx,js,jsx}\"",
    "lint": "eslint --cache --fix src/**",
    "api-extractor": "api-extractor run --local --verbose",
    "generate-docs": "api-documenter markdown --input-folder temp --output-folder docs/api && rm -rf temp",
    "test": "vitest run",
    "generate-notices": "generate-license-file --input package.json --output ./THIRD-PARTY-NOTICES --overwrite",
    "prepare": "pnpm build"
  },
  "bin": {
    "pages": "dist/bin/spawn.js"
  },
  "files": [
    "dist/**/*",
    "THIRD-PARTY-NOTICES"
  ],
  "dependencies": {
    "ansi-to-html": "^0.7.2",
    "browser-or-node": "^2.1.1",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "escape-html": "^1.0.3",
    "express": "^4.18.3",
    "fs-extra": "^11.2.0",
    "get-port": "^7.0.0",
    "glob": "^10.3.10",
    "latest-version": "^9.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "module-from-string": "^3.3.0",
    "open": "^10.1.0",
    "ora": "^8.0.1",
    "picocolors": "^1.0.0",
    "pretty-ms": "^9.0.0",
    "postcss": "^8.4.35",
    "postcss-nested": "^6.0.1",
    "prompts": "^2.4.2",
    "rollup": "^4.13.0",
    "ts-morph": "^22.0.0",
    "typescript": "^5.4.2",
    "vite-plugin-node-polyfills": "0.17.0",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.23.38",
    "@microsoft/api-extractor": "^7.42.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/escape-html": "^1.0.4",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.43",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "4.0.9",
    "@types/lodash": "^4.17.0",
    "@types/mime-types": "^2.1.4",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^20.11.28",
    "@types/prompts": "^2.4.9",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/ws": "^8.5.10",
    "esbuild": "^0.20.2",
    "generate-license-file": "^3.0.1",
    "glob": "^10.3.10",
    "jsdom": "24.0.0",
    "minimatch": "^9.0.3",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.2.0",
    "react-dom": "^17.0.2 || ^18.2.0",
    "vite": "^4.3.0 || ^5.0.2"
  }
}
