{
  "name": "@yext/pages",
  "version": "1.0.0-rc.0",
  "description": "The default React development toolchain provided by Yext",
  "author": "sumo@yext.com",
  "license": "BSD-3-Clause",
  "keywords": [
    "Yext",
    "React"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./vite-plugin": "./dist/vite-plugin/plugin.js",
    "./util": "./dist/util/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/src/index.d.ts"
      ],
      "vite-plugin": [
        "dist/types/src/vite-plugin/index.d.ts"
      ],
      "util": [
        "dist/types/src/util/index.d.ts"
      ]
    }
  },
  "engines": {
    "node": "^17 || ^18 || ^19"
  },
  "scripts": {
    "watch": "node src/bundler --watch",
    "build:js": "node src/bundler.js",
    "build:types": "tsc",
    "build": "pnpm build:js && pnpm build:types",
    "fmt": "prettier --write --cache \"src/**/**.{ts,tsx,js,jsx}\"",
    "lint": "eslint --cache --fix src/**",
    "api-extractor": "api-extractor run --local --verbose",
    "generate-docs": "api-documenter markdown --input-folder temp --output-folder docs/api && rm -rf temp",
    "test": "jest",
    "generate-notices": "generate-license-file --input package.json --output ./THIRD-PARTY-NOTICES --overwrite",
    "prepare": "pnpm build"
  },
  "bin": {
    "pages": "dist/bin/spawn.js"
  },
  "files": [
    "dist/**/*",
    "THIRD-PARTY-NOTICES"
  ],
  "dependencies": {
    "ansi-to-html": "^0.7.2",
    "browser-or-node": "^2.1.1",
    "chalk": "^5.3.0",
    "cli-spinners": "^2.9.0",
    "commander": "^11.0.0",
    "cross-fetch": "^4.0.0",
    "escape-html": "^1.0.3",
    "esm-module-paths": "^1.1.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "get-port": "^7.0.0",
    "glob": "^10.3.3",
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "module-from-string": "^3.3.0",
    "open": "^9.1.0",
    "ora": "^7.0.1",
    "picocolors": "^1.0.0",
    "pretty-ms": "^8.0.0",
    "prompts": "^2.4.2",
    "rollup": "^3.28.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-node-polyfills": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@microsoft/api-documenter": "^7.22.33",
    "@microsoft/api-extractor": "^7.36.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.3",
    "@types/js-yaml": "4.0.5",
    "@types/lodash": "^4.14.197",
    "@types/mime-types": "^2.1.1",
    "@types/minimatch": "^5.1.2",
    "@types/prompts": "^2.4.4",
    "@types/node": "^20.4.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/ws": "^8.5.5",
    "babel-loader": "^9.1.3",
    "esbuild": "^0.19.0",
    "generate-license-file": "^2.0.0",
    "glob": "^10.3.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "minimatch": "^9.0.3",
    "ts-jest": "^29.1.1",
    "ts-jest-mock-import-meta": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.2.0",
    "react-dom": "^17.0.2 || ^18.2.0"
  },
  "jest": {
    "rootDir": "..",
    "collectCoverageFrom": [
      "<rootDir>/pages/src/**/*.{ts,tsx,js}"
    ],
    "verbose": true,
    "testMatch": [
      "<rootDir>/pages/src/**/*.test.{ts,tsx,js}"
    ],
    "transform": {
      "\\.[jt]sx?$": [
        "ts-jest",
        {
          "tsconfig": {
            "allowSyntheticDefaultImports": true,
            "esModuleInterop": true,
            "allowJs": true,
            "target": "ESNext",
            "jsx": "react"
          },
          "diagnostics": {
            "ignoreCodes": [
              1343
            ]
          },
          "astTransformers": {
            "before": [
              {
                "path": "ts-jest-mock-import-meta",
                "options": {
                  "metaObjectReplacement": {
                    "url": "JEST"
                  }
                }
              }
            ]
          }
        }
      ]
    },
    "moduleNameMapper": {
      "^(.+)\\.js$": "$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    }
  },
  "overrides": {
    "webpack": "^5.52.0"
  },
  "setupFiles": [
    "dotenv/config"
  ]
}
