{
  "name": "@yext/yext-sites-scripts",
  "version": "0.0.18",
  "description": "The default React development toolchain provided by Yext",
  "keywords": [
    "Yext",
    "React"
  ],
  "author": "Yext",
  "private": "true",
  "type": "module",
  "types": "dist/types/yext-sites-scripts/src/index.d.ts",
  "main": "dist/yext-sites-scripts/src/index.js",
  "engines": {
    "node": ">=17"
  },
  "scripts": {
    "watch": "node src/bundler --watch",
    "build": "node src/bundler.js",
    "types": "tsc && npm run api-extractor && npm run generate-docs",
    "fmt": "prettier -w .",
    "release": "npm run build && npm run types",
    "lint": "eslint .",
    "api-extractor": "api-extractor run --local --verbose",
    "generate-docs": "api-documenter markdown --input-folder temp --output-folder docs/api && rm -rf temp",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs --port 8080",
    "test": "jest"
  },
  "bin": {
    "yss": "dist/yext-sites-scripts/src/bin/yext-sites-scripts.js"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@yext/vite-plugin-yext-sites-ssg": "^0.0.7",
    "ansi-to-html": "^0.7.2",
    "cli-spinners": "^2.6.1",
    "commander": "^9.1.0",
    "cross-fetch": "^3.1.5",
    "escape-html": "^1.0.3",
    "esm-module-paths": "^1.1.1",
    "express": "^4.17.3",
    "ink": "^3.2.0",
    "open": "^8.4.0",
    "shelljs": "^0.8.5",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@microsoft/api-documenter": "^7.17.9",
    "@microsoft/api-extractor": "^7.22.2",
    "@types/babel__core": "^7.1.18",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.28",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.10",
    "@types/shelljs": "^0.8.11",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "vitepress": "^0.22.3"
  },
  "peerDependencies": {
    "@vitejs/plugin-react": "^1.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "vite": "^2.9.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts"
    ],
    "verbose": true,
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  }
}
